# Test Environment Variables
# Copy this file to .env.test and fill in with your test credentials

# Test User Credentials
# These should be actual users in your test/staging Cognito user pool
# Cognito requires USERNAME (not email) for login
TEST_ADMIN_USERNAME=admin
TEST_ADMIN_EMAIL=admin@test.com
TEST_ADMIN_PASSWORD=YourSecureTestPassword123!

TEST_INSTRUCTOR_USERNAME=instructor
TEST_INSTRUCTOR_EMAIL=instructor@test.com
TEST_INSTRUCTOR_PASSWORD=YourSecureTestPassword123!

TEST_USER_USERNAME=user
TEST_USER_EMAIL=user@test.com
TEST_USER_PASSWORD=YourSecureTestPassword123!

# Database (usually same as development for local testing)
DATABASE_URL=your_test_database_url_here

# AWS Cognito (Test/Staging Environment)
COGNITO_USER_POOL_ID=your_test_user_pool_id
COGNITO_CLIENT_ID=your_test_client_id
COGNITO_CLIENT_SECRET=your_test_client_secret
AWS_REGION=us-east-1

# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_test_nextauth_secret

# Anthropic AI (for blueprint extraction tests)
ANTHROPIC_API_KEY=sample_key_replace_with_real_key
